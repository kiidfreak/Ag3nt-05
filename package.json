{
  "name": "agent-labs-os",
  "version": "0.1.0",
  "description": "The Canva for Agents â€” A modular lab OS that makes multi-agent systems discoverable, composable, and deployable across real-world domains",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "agents/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:gateway\" \"npm run dev:studio\" \"npm run dev:registry\" \"npm run dev:dashboard\"",
    "dev:gateway": "cd apps/gateway && npm run dev",
    "dev:studio": "cd apps/studio && npm run dev",
    "dev:registry": "cd apps/registry && npm run dev",
    "dev:dashboard": "cd apps/dashboard && npm run dev",
    "build": "npm run build:packages && npm run build:apps",
    "build:packages": "npm run build --workspaces --if-present",
    "build:apps": "npm run build --workspace=apps/*",
    "test": "npm run test --workspaces --if-present",
    "test:e2e": "cypress run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm install && npm run setup:packages && npm run setup:apps",
    "setup:packages": "npm install --workspace=packages/*",
    "setup:apps": "npm install --workspace=apps/*",
    "clean": "npm run clean:node_modules && npm run clean:dist",
    "clean:node_modules": "rm -rf node_modules apps/*/node_modules packages/*/node_modules agents/*/node_modules",
    "clean:dist": "rm -rf apps/*/dist packages/*/dist agents/*/dist"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "cypress": "^13.3.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/agent-labs-os.git"
  },
  "keywords": [
    "agents",
    "ai",
    "blockchain",
    "coral-protocol",
    "solana",
    "multi-agent-systems",
    "compliance",
    "trust",
    "finance"
  ],
  "author": "Agent Labs Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/agent-labs-os/issues"
  },
  "homepage": "https://agent-labs-os.vercel.app"
}